    "express": "^4.13.2",
    "express-graphql": "^0.5.1",
    "express-session": "^1.11.3",
    "github4": "^0.6.0",
    "graphql": "^0.5.0",
    "graphql-relay": "^0.4.1",
    "immutable": "^3.8.1",
