    "url": "https://github.com/CodeNow/eru/issues"
  },
  "homepage": "https://github.com/CodeNow/eru#readme",
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach"
    ],
    "parser": "babel-eslint",
    "ignore": [
      "public/*"
    ]
  },
  "dependencies": {
    "101": "^1.5.0",
    "amqplib": "^0.4.1",
